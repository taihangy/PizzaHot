<p id="notice"><%= notice %></p>

<h1>Listing Feedback</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Feedback</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.feedback %></td>
        <td><%= link_to 'Show', contact %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


